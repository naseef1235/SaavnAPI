{
  "rewrites": [
    {
      "source": "/",
      "destination": "/api"
    },
    {
      "source": "/search",
      "destination": "/api/search"
    },
    {
      "source": "/song",
      "destination": "/api/song"
    },
    {
      "source": "/lyrics",
      "destination": "/api/lyrics"
    },
    {
      "source": "/albumsearch",
      "destination": "/api/albumsearch"
    },
    {
      "source": "/album",
      "destination": "/api/album"
    },
    {
      "source": "/link",
      "destination": "/api/link"
    },
    { "source": "/docs(.*)", "destination": "/api/docs" }
  ]
}
